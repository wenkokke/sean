domain_size = 2

noun_phrase = \A.A _:e

noun_1      = _:et
noun_2      = _:eet
noun_2      = \P.\y.P (\x._:eet x y)

and         = \A.\B.\x.A x /\ B x

the         = \A.\B.B (ix.A x)
some        = \A.\B.?x.A x /\ B x
every       = \A.\B.!x.A x => B x

verb_phrase = _:et

verb_1      = _:et
verb_2      = \P.\z.P (\x._:eet z x)
verb_3      = \P.\Q.\z.P (\x.Q (\y._:eeet z x y))

james       = 0
dog         = {0,1}
female      = {1}
bitch       = \x.female x /\ dog x
ran         = {1}
liked       = {(0,1)}

text        = some (noun_1 [ dog ]) ( (verb_2 [ likes ]) (every (noun_1 [ bitch ])) )
test        = noun_phrase [ james ] 

